<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.groceteria">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.SplashActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.OrderActivity" />
        <activity android:name=".Activity.FavouritesActivity" />
        <activity android:name=".Activity.SignInActivity" />
        <activity android:name=".Activity.CartActivity" />
        <activity android:name=".Activity.WalletActivity" />
        <activity android:name=".Activity.TermsConditionsActivity" />
        <activity android:name=".Activity.SettingsActivity" />
        <activity android:name=".Activity.RateUsActivity" />
        <activity android:name=".Activity.NotificationActivity" />
    </application>

</manifest>